{
  "name": "@capnet/shared",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "clean": "node -e \"const fs=require('fs');if(fs.existsSync('dist'))fs.rmSync('dist',{recursive:true})\"",
    "build": "npm run clean && tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tweetnacl": "1.0.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
